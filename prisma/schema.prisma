generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model User {
    id     String @id @default(auto()) @map("_id") @db.ObjectId
    userId Int    @unique
    Staff  Staff?
}

model Staff {
    id       String  @id @default(auto()) @map("_id") @db.ObjectId
    userId   Int     @unique
    isActive Boolean
    User     User    @relation(fields: [userId], references: [userId])
}
